{
    "manifest_version": 3,
  
    "name": "JS scraper Chrome Plugin Original",
    "description": "This extension runs JS code.",
    "version": "1.0",
  
    "action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "background.service_worker": {
        "scripts": ["./background.js"]
    },
    "host_permissions": [
        "*://*/*",
        "http://rinkworks.com/slapdash/broken.shtml"
    ],
    
    "permissions": [
      "tabs",
      "*://*/*",
      "https://api.sendgrid.com/v3/mail/send",
      "https://ajax.googleapis.com/",
      "https://api.sendgrid.com/*"
    ]  
    
}
