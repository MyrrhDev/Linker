{
    "manifest_version": 2,
  
    "name": "JS scraper Chrome Plugin Original",
    "description": "This extension runs JS code.",
    "version": "1.0",
  
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "tabs",
      "<all_urls>", 
      "*://*/*",
      "https://ajax.googleapis.com/*",
      "https://www.google.com/*",
      "http://github.com/*"
      
    ]
  }